<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace:绑定一个指定的dao或者mapper接口,用于跟接口连接-->
<mapper namespace="com.swj.dao.UserMapper">
    <!--id：接口的方法名字，用于指定某一个方法；resultType：返回值的类型-->
    <select id="getUserList" resultType="com.swj.domain.Tbuser">
    select * from tbuser
  </select>
</mapper>